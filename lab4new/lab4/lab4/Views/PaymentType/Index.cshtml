@model IEnumerable<PaymentType>
<div>
    <table class="table">
        <tr>
            <th>Id</th>
            <th>Название</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
            </tr>
        }
    </table>
</div>